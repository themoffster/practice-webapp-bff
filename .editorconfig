# This file is managed by the jenkins-project-standardisation-builds project. Do not edit.

root = true

[*]
charset = utf-8
end_of_line = lf
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[*.dot]
indent_size = 2

[*.gradle]
indent_size = 4

[*.groovy]
indent_size = 4

[*.java]
indent_size = 4

[*.js]
indent_size = 2

[*.json]
indent_size = 2

[*.sql]
indent_size = 2

[*.yml]
indent_size = 2

[Jenkinsfile]
indent_size = 4
